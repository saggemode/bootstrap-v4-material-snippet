			},
			{
				"language": "blade",
				"path": "./snippets/snippets.json"
			},
			{
				"language": "vue",
				"path": "./snippets/snippets.json"
			},
			{
				"language": "smarty",
				"path": "./snippets/snippets.json"
			},
			{
				"language": "cfml",
				"path": "./snippets/snippets.json"
			}
        ]
