{
    "name": "bs4-material-snippet",
    "displayName": "bs4-material-snippet",
    "description": "Simple bootstrap 4 Material snippet generator",
    "icon": "images/icon.png",
    "version": "1.0.0",
    "engines": {
        "vscode": "^1.37.0"
    },
    "repository": {
		"type": "git",
		"url": "https://github.com/saggemode/bootstrap-v4-material-snippet"
	},
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "html",
                "path": "./snippets/snippets.json"
